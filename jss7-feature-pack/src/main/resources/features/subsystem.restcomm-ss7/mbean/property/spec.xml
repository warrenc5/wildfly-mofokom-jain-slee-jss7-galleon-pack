<?xml version="1.0" ?>

<feature-spec xmlns="urn:jboss:galleon:feature-spec:1.0" name="subsystem.restcomm-ss7.mbean.property">
    <annotation name="jboss-op">
        <elem name="name" value="add"/>
        <elem name="op-params" value="name,value"/>
        <!--
        <elem name="addr-params" value=""/>
                -->
    </annotation>
    <annotation name="feature-branch">
        <elem name="id" value="property"/>
    </annotation>
    <refs>
        <ref feature="profile" nillable="true"/>
    </refs>
    <params>
        <param name="name" feature-id="true"/>
        <param name="value" feature-id="true"/>
        <param name="profile" feature-id="true" default="GLN_UNDEFINED"/>
    </params>
</feature-spec>